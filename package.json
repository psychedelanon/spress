{
  "name": "spress-chess",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=20 <21"
  },
  "packageManager": "npm@10.9.2",
  "scripts": {
    "dev:bot": "nodemon --exec ts-node src/server.ts",
    "dev:web": "cd webapp && npm run dev -- --host 0.0.0.0 --port 5173 --open",
    "dev": "concurrently -k \"npm run dev:bot\" \"npm run dev:web\"",
    "build:backend": "tsc src/server.ts src/wsHub.ts src/types.ts src/engine/ai.ts src/engine/stockfish.ts src/store/db.ts src/telechess/commands.ts src/telechess/GameSession.ts src/utils/ensureHttps.ts --outDir dist --module commonjs --target ES2022 --esModuleInterop --resolveJsonModule --skipLibCheck",
    "build:webapp": "cd webapp && npm run build",
    "build": "npm run build:backend && npm run build:webapp",
    "start": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chess.js": "^1.4.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "telegraf": "^4.16.3",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
